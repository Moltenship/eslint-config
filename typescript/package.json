{
  "name": "@moltenship/eslint-config-typescript",
  "description": "Sharable ESLint config for typescript",
  "version": "1.1.1",
  "repository": "moltenship/eslint-config",
  "author": "Timofey Maximov",
  "license": "ISC",
  "keywords": [
    "eslint",
    "eslint-config",
    "linter",
    "typescript"
  ],
  "scripts": {
    "build": "vite build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-define-config": "^1.20.0",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0"
  },
  "peerDependencies": {
    "eslint": "^8.38.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-perfectionist": "^1.3.0",
    "eslint-plugin-vitest": "^0.2.6"
  }
}
